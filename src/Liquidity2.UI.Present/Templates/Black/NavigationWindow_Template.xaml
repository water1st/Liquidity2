    <ControlTemplate x:Key="NavigationWindow_Template">
        <ControlTemplate.Resources>
            <Style x:Key="NavigationButton" TargetType="Button">
                <Setter Property="Height" Value="60" />
                <Setter Property="Padding" Value="19,13" />
                <Setter Property="Background" Value="#11121A" />
                <Setter Property="BorderBrush" Value="Transparent" />
                <Setter Property="Template">
                    <Setter.Value>
                        <ControlTemplate TargetType="Button">
                            <Grid>
                                <Border Name="BackgroundOver" Opacity="0" Background="#162E60" />
                                <ContentPresenter VerticalAlignment="Center" HorizontalAlignment="Center" Name="MainContent" />
                            </Grid>
                            <ControlTemplate.Triggers>
                                <Trigger Property="IsMouseOver" Value="True">
                                    <Trigger.EnterActions>
                                        <BeginStoryboard>
                                            <Storyboard>
                                                <DoubleAnimation Storyboard.TargetName="BackgroundOver" Storyboard.TargetProperty="Opacity" To="1" Duration="0:0:0.2" />
                                            </Storyboard>
                                        </BeginStoryboard>
                                    </Trigger.EnterActions>
                                    <Trigger.ExitActions>
                                        <BeginStoryboard>
                                            <Storyboard>
                                                <DoubleAnimation Storyboard.TargetName="BackgroundOver" Storyboard.TargetProperty="Opacity" To="0" Duration="0:0:0.2" />
                                            </Storyboard>
                                        </BeginStoryboard>
                                    </Trigger.ExitActions>
                                </Trigger>
                            </ControlTemplate.Triggers>
                        </ControlTemplate>
                    </Setter.Value>
                </Setter>
            </Style>
            <Style x:Key="NavigationButtonCanvas" TargetType="Canvas">
                <Setter Property="HorizontalAlignment" Value="Center" />
                <Setter Property="VerticalAlignment" Value="Top" />
                <Setter Property="Width" Value="18" />
                <Setter Property="Height" Value="18" />
            </Style>
            <Style x:Key="NavigationButtonPath" TargetType="Path">
                <Setter Property="Fill" Value="#FFFFFF" />
                <Setter Property="Opacity" Value="0.800000012" />
            </Style>
            <Style x:Key="NavigationButtonTextBlock" TargetType="TextBlock">
                <Setter Property="Margin" Value="0,4,0,0" />
                <Setter Property="FontSize" Value="10" />
                <Setter Property="Foreground" Value="#FFFFFF" />
            </Style>
        </ControlTemplate.Resources>
